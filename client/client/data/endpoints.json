{
  "partitions": [{
    "defaults": {
      "hostname": "{service}:{port}/{endpoint}{dnsSuffix}"
    },
    "dnsSuffix" : "",
    "partition" : "aws",
    "partitionName" : "AWS Standard",
    "protocols" : ["http"],
    "services" : {
      "writer" : {
        "defaults": {
          "hostname": "minio:{port}"
        },
        "port": "9000",
        "endpoints": {}
      },
      "forecast": {
        "defaults" : {
          "hostname" : "api:{port}/{service}/{endpoint}{dnsSuffix}"
        },
        "port": "80",
        "endpoints": {}
      },
      "login": {
        "defaults": {
          "hostname": "api:{port}/{service}{dnsSuffix}"
        },
        "port": "80",
        "endpoints": {}
      },
      "authenticator": {
        "defaults": {
          "hostname" : "api:{port}/users/me"
        },
        "port": 80,
        "endpoints": {}
      }
    }
  }]
}
